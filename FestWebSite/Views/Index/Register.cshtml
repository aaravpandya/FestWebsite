                    <div class="post-comment">
                        <div class="clearfix"></div>
                        <div class="reply-post clearafix">
                            <h6 class="reply-title">Registration Form</h6>
                            <div class="comment-reply-form clearfix">

                                <form asp-action="PostDataAsync" asp-controller="Index" method="post">
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label asp-for="Name" class="registrattion-title">Name</label>
                                                <input style="color: white;" maxlength="256" asp-for="Name" type="text" required>
                                            </div>
                                        </div>

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label asp-for="UniversityName" class="registrattion-title">University</label>
                                                <input style="color: white;" maxlength="256" asp-for="UniversityName" type="text" required>
                                            </div>
                                        </div>


                                        <div class="col-12">
                                            <div class="form-group">
                                                <label asp-for="Email" class="registrattion-title">Email</label>
                                                <input style="color: white;" asp-for="Email" type="email" required>
                                            </div>
                                        </div>

                                        <div class="col-lg-7">
                                            <div class="form-group">
                                                <label asp-for="ContactNumber" class="registrattion-title">Contact Number</label>
                                                <input style="color: white;" type="tel" pattern="[789][0-9]{9}" onkeypress="return (event.charCode >= 48 && event.charCode <= 57)"
                                                       maxlength="10" asp-for="ContactNumber"
                                                       required>
                                            </div>
                                        </div>

                                        <div class="col-lg-7">
                                            <div class="form-group">
                                                <label asp-for="NumberOfParticipants">Number of Participants</label>
                                                <input style="color: white;" asp-for="NumberOfParticipants" type="number" required>
                                            </div>
                                        </div>

                                        <div class="col-lg-12">
                                            <div class="row">
                                                <div class="col-lg-4">
                                                    <div class="form-group">
                                                        <label>Accomodation</label>
                                                    </div>
                                                </div>
                                                @*<div class="col-lg-2">
                                                        <div class="form-group">
                                                            <label class="radio-inline">
                                                                Yes
                                                                <div style="margin-top: -2vh;">
