                                                @Html.RadioButtonFor(model => model.Accomodation, "true", new { id = "female" })
                                                @Html.Label("yes", "Yes")
                                            </div>
                                        </div>

                                        <div class="col-lg-7">
                                            <div class="form-group">
                                                <label asp-for="Events" class="registrattion-title">Events</label>
                                                <select class="custom-select mar" asp-for="Events"
